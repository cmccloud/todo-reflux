  },
  "author": "Henrik Andersson",
  "license": "MIT",
  "devDependencies": {
    "bounce.js": "^0.8.1",
    "jest": "^0.1.37",
    "jest-cli": "^0.2.2",
    "jshint": "^2.6.0",
    "jshint-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "jsxhint-loader": "^0.2.0",
    "less": "^2.3.1",
    "react": "^0.12.2",
    "reflux": "^0.2.4",
    "superagent": "^0.21.0",
    "underscore": "^1.7.0",
    "webpack": "^1.5.3"
